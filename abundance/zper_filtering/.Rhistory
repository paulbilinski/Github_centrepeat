calcFreqs <- function(data,pops,popList){
frqs <- matrix(NA,nrow=2,ncol=length(pops))
row.names(frqs) <- paste(data[1],c("_1","_-1"))
colnames(frqs) <- pops
for(i in 1:length(pops)){
missing <- length(which(is.na(data[popList[[i]]])))
het <-  length(which(data[popList[[i]]] == 0))
high <- 2*length(which(data[popList[[i]]] == 1)) + het
low <- 2*length(which(data[popList[[i]]] == -1)) + het
frqs[1,i] <- high
frqs[2,i] <- low
}
return(frqs)
}
